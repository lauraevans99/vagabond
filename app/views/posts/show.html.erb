<div class="col-md-6 col-md-offset-3 text-center">
	

	<% @post.each do |p| %>
	<div class="well">
		<h4><%= p.title %></h4>
		<h5><%= p.body %></h5>
		<%= button_to "delete", {:controller => :posts,
	      :action => 'destroy', :id => p.id }, :method => :delete %>
	    <button type="button" class="btn btn-default">
		<a href="/users/5/posts/<%=p.id%>/edit">Update</a></button>
	</div>
	<% end %>

	<button type="button" class="btn btn-default">
		<a href="/users/<%= @user.id %>/posts">Back to Posts</a>
	</button>

</div>